<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0"
      prefWidth="700.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.isky.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <children>
        <Pane prefHeight="600.0" prefWidth="700.0">
            <children>
                <Button fx:id="selJpgFolderBtn" layoutX="104.0" layoutY="75.0" onAction="#onHelloButtonClick"
                        prefHeight="29.0" prefWidth="149.0" style="-fx-background-color: green;" text="选择文件夹:"/>

                <Label fx:id="jpgFolderLabel" layoutX="351.0" layoutY="85.0"/>
                <ListView fx:id="imgListView" layoutX="39.0" layoutY="163.0" prefHeight="258.0" prefWidth="573.0"/>
                <Button fx:id="hcPdtBtn" layoutX="154.0" layoutY="458.0" mnemonicParsing="false" onAction="#hcPdtClick"
                        prefHeight="36.0" prefWidth="99.0" style="-fx-background-color: green;" text="合成"/>
                <Button fx:id="clearBtn" layoutX="397.0" layoutY="458.0" mnemonicParsing="false"
                        onAction="#clearBtnClick" prefHeight="36.0" prefWidth="99.0" style="-fx-background-color: red;"
